<Map background-color="transparent" srs="+init=epsg:3857" minimum-version="0.7.2">

  <Style name="lulc">
    <Rule>
      <RasterSymbolizer>
        <RasterColorizer default-mode="discrete" default-color="transparent" epsilon="0.001">
          <stop value="1" color="#377eb8" label="Water"/>
          <stop value="2" color="#999999" label="Developed"/>
          <stop value="3" color="#984ea3" label="Plantation"/>
          <!--<stop value="4" color="rgb(209,155,255)" label=""/>-->
          <stop value="5" color="#e41a1c" label="Barren"/>
          <stop value="6" color="#a65628" label="Forest"/>
          <stop value="7" color="#4daf4a" label="Grassland"/>
          <stop value="8" color="#ffff33" label="Agriculture"/>
          <stop value="9" color="#f781bf" label="Wetland"/>
          <stop value="10" color="#ff7f00" label="Shrubland"/>
        </RasterColorizer>
      </RasterSymbolizer>
    </Rule>
  </Style>

</Map>
